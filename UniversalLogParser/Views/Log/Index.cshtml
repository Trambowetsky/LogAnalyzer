@model IEnumerable<UniversalLogParser.Models.LogEntry>

<h2>Log Entries</h2>

<table class="table">
    <thead>
    <tr>
        <th>Date</th>
        <th>Level</th>
        <th>Message</th>
    </tr>
    </thead>
    <tbody>
    @foreach (var log in Model)
    {
        <tr>
            <td>@log.Date</td>
            <td>@log.Level</td>
            <td>@log.Message</td>
        </tr>
    }
    </tbody>
</table>